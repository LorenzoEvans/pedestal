(require 'core)
core/respond-hello
(core/respond-hello {})
(require 'core)
(core/respond-hello {})
(require 'core)
(core/respond-hello {})
(exit)
(require 'core)
(core/respond-hello {})
(require :reload 'core)
(require '[io.pedestal.http.route :as route])
(route/try-routing-for core/routes :prefix-tree "/greet" :get)
(route/try-routing-for core/routes :prefix-tree "/greet" :put)
(require :reload 'core)
(exit)
(require :reload 'core)
